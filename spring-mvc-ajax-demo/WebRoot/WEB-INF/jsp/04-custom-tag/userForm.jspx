<html xmlns:c="http://java.sun.com/jsp/jstl/core"  xmlns:spring="http://www.springframework.org/tags" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:form="http://www.springframework.org/tags/form"xmlns:html="urn:jsptagdir:/WEB-INF/tags/html" xmlns:ajax="urn:jsptagdir:/WEB-INF/tags/ajax">	<jsp:output doctype-root-element="HTML" doctype-system="about:legacy-compat" />	<jsp:directive.page contentType="text/html;charset=UTF-8" />  	<head>		<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1"/>		<title>Add User</title>		<html:stylesheet url="/bootstrap/css/bootstrap.min.css" />		<html:script url="/js/jquery.js"/>	</head>	<body>		<div class="container" style="padding-top: 50px;">			<jsp:include page="../menu.jspx"/>					<html:form modelAttribute="user"  id="add-user-form" formUrl="/userAjaxCustomTag.htm">					<html:inputField name="firstName" label="First name" />					<html:inputField name="lastName" label="Last name" />					<html:inputField name="age" label="Age" />					<div class="form-actions">						<button type="submit" class="btn btn-primary">Save changes</button>						<button type="reset" class="btn">Cancel</button>					</div>			</html:form>		</div>			<ajax:formPartialRefresh validateUrl="/userAjaxBootstrap.json" formName="add-user-form"/>	</body></html>